{
  "name": "anandapp",
  "displayName": "anandapp",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "homepage": "./",
  "scripts": {
    "start": "npm run remove:www && react-scripts start",
    "build": "npm run remove:www && react-scripts build",
    "build:cordova": "npm run remove:www && react-scripts build && mv build www",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "remove:www": "rm -rf www",
    "deploy-app": "cordova emulate android"
  },
  "dependencies": {
    "cordova-android": "^8.1.0",
    "cordova-browser": "^5.0.3",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-whitelist": "^1.3.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android"
    ]
  }
}